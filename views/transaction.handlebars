
<section class="column bg-dark">
    <section class="transaction bg-dark">
        {{#each transactions as |transaction|}}
            <div class="card-body bg-dark text-white font-weight-bold text-center">
                <h3>{{transaction.type}}</h3>
                <h5>{{transaction.amount}}</h5>
                <h6>{{transaction.date}}</h6>
            </div>
        {{/each}}
    </section>
</section>
